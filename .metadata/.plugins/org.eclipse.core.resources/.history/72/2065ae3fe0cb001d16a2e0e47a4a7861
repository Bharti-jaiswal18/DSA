
public class sortingA {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] a= {38,52,9,16,6,62,13};
		int min; int temp=0;
		for(int i=0;i<a.length;i++)
		{
			min=i;
			for(int j=i+1;j<a.length;j++)
			{if(a[j]<a[i]) {
				min=j;                                              
				
			}
				temp=a[i];
				a[i]=a[min];
				a[min]=temp;
			}
		}
		

	}

}
